(* Notazione per i nomi *****************************************************)

notation "hvbox( 𝕍 )"
  non associative with precedence 70
  for @{ 'CategoriaV }.

notation "hvbox( 𝗑[ term 46 p ] )"
  non associative with precedence 70
  for @{ 'Nome $p }.

notation "hvbox( 𝖿 )"
  non associative with precedence 70
  for @{ 'NomeF }.

notation "hvbox( 𝗀 )"
  non associative with precedence 70
  for @{ 'NomeG }.

notation "hvbox( 𝗑 )"
  non associative with precedence 70
  for @{ 'NomeX }.

notation "hvbox( 𝗒 )"
  non associative with precedence 70
  for @{ 'NomeY }.

notation "hvbox( 𝗓 )"
  non associative with precedence 70
  for @{ 'NomeZ }.
