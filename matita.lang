    <regex>\|</regex>
  </pattern-item>
  <pattern-item _name = "Command ]" style = "Keyword">
    <regex>\]</regex>
  </pattern-item>
  <pattern-item _name = "Command {" style = "Keyword">
    <regex>\{</regex>
  </pattern-item>
  <pattern-item _name = "Command }" style = "Keyword">
    <regex>\}</regex>
  </pattern-item>
  <pattern-item _name = "Notation ast mark" style = "Keyword">
    <regex>@</regex>
  </pattern-item>
  <pattern-item _name = "Notation meta mark" style = "Keyword">
    <regex>\$</regex>
  </pattern-item>

